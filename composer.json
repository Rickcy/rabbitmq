{
  "name": "rickcy/yii2-rabbitmq",
  "description": "Wrapper based on php-amqplib to incorporate messaging in your Yii2 application via RabbitMQ.",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "rabbitmq",
    "rabbit",
    "php-rabbit",
    "mq"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Evgeniy Kudenko",
      "email": "kuden.and.ko@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "yiisoft/yii2": "^2.0.32",
    "php-amqplib/php-amqplib": "^2",
    "ext-json": "*"
  },
  "minimum-stability": "dev",
  "require-dev": {
    "roave/security-advisories": "dev-master"
    ,
    "phpunit/phpunit": "^8",
    "php-coveralls/php-coveralls": "^2"
  },
  "autoload": {
    "psr-4": {
      "rickcy\\rabbitmq\\": ""
    }
  },
  "extra": {
    "bootstrap": "rickcy\\rabbitmq\\DependencyInjection"
  }
}
