{
  "name": "rickcy/yii2-rabbitmq",
  "description": "Wrapper based on php-amqplib to incorporate messaging in your Yii2 application via RabbitMQ.",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "rabbitmq",
    "rabbit",
    "php-rabbit",
    "mq"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Evgeniy Kudenko",
      "email": "kuden.and.ko@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "yiisoft/yii2": "~2.0.0",
    "php-amqplib/php-amqplib": "^2.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "php-coveralls/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "rickcy\\rabbitmq\\": ""
    }
  },
  "extra": {
    "bootstrap": "rickcy\\rabbitmq\\DependencyInjection"
  }
}
